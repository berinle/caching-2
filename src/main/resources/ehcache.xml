<ehcache>

    <diskStore path="java.io.tmpdir"/>


    <defaultCache
        maxElementsInMemory="10000"
        eternal="false"
        timeToIdleSeconds="120"
        timeToLiveSeconds="120"
        overflowToDisk="true"
        />

    <cache name="domain.Fiddle" maxElementsInMemory="20" eternal="false" timeToIdleSeconds="5" timeToLiveSeconds="10" overflowToDisk="false"/>

</ehcache>
